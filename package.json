{
	"name": "survey-tool",
	"private": true,
	"version": "0.0.0",
	"type": "module",
	"homepage": "/",
	"main": "/src/main.tsx",
	"scripts": {
		"dev": "vite",
		"format": "prettier . --write",
		"build": "tsc -b ",
		"build:staging": "tsc -b tsconfig.node.json && tsc && vite build --mode staging",
		"build:production": "tsc -b tsconfig.node.json && tsc && vite build --mode production",
		"lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
		"lint:fix": "eslint . --ext .ts,.tsx --fix --report-unused-disable-directives --max-warnings 0",
		"preview": "vite preview"
	},
	"dependencies": {
		"@reduxjs/toolkit": "^2.3.0",
		"@types/react-toastify": "^4.1.0",
		"axios": "^1.7.7",
		"clsx": "^2.1.1",
		"env-cmd": "^10.1.0",
		"js-cookie": "^3.0.5",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-error-boundary": "^4.0.13",
		"react-helmet-async": "^2.0.5",
		"react-icons": "^5.3.0",
		"react-redux": "^9.1.2",
		"react-router-dom": "^6.23.1",
		"redux-persist": "^6.0.0",
		"uuid": "^10.0.0",
		"vite-plugin-sass": "^0.1.0",
		"vite-plugin-sass-dts": "^1.3.22"
	},
	"devDependencies": {
		"@stylistic/eslint-plugin-js": "^2.3.0",
		"@types/js-cookie": "^3.0.6",
		"@types/node": "^20.14.1",
		"@types/node-sass": "^4.11.7",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.1",
		"@types/uuid": "^10.0.0",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"@vitejs/plugin-react-refresh": "^1.3.6",
		"@vitejs/plugin-react-swc": "^3.5.0",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react": "^7.37.1",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.6",
		"lightningcss": "^1.25.1",
		"prettier": "^3.3.0",
		"sass": "^1.79.5",
		"terser": "^5.31.0",
		"typescript": "^5.2.2",
		"vite": "^5.2.0"
	},
	"description": "",
	"repository": {
		"type": "git",
		"url": "surveytool"
	},
